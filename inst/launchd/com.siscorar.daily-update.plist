<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.siscorar.daily-update</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/Rscript</string>
        <string>inst/scripts/daily_update.R</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/eleon/github/rsiscorar</string>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>6</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/eleon/Library/Logs/siscorar-daily-update.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/eleon/Library/Logs/siscorar-daily-update.log</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/opt/homebrew/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>
</dict>
</plist>
